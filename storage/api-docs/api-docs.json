{
    "openapi": "3.0.0",
    "info": {
        "title": "BiznisBox API",
        "description": "BiznisBox API Documentation",
        "contact": {
            "email": "info@biznisbox.com"
        },
        "license": {
            "name": "AGPL-3.0-or-later",
            "url": "https://www.gnu.org/licenses/agpl-3.0.en.html"
        },
